{
  "name": "@echristian/eslint-config",
  "version": "0.0.20",
  "description": "ericc-ch ESLint config",
  "keywords": [
    "eslint-config"
  ],
  "homepage": "https://github.com/ericc-ch/eslint-config",
  "bugs": "https://github.com/ericc-ch/eslint-config/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ericc-ch/eslint-config.git"
  },
  "author": "Erick Christian <erickchristian48@gmail.com>",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "default": "./dist/main.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint .",
    "prepack": "pnpm run build",
    "prepare": "simple-git-hooks",
    "release": "bumpp && pnpm publish --access public"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "pnpm run lint --fix"
  },
  "dependencies": {
    "@eslint/js": "^9.20.0",
    "@eslint/json": "^0.10.0",
    "@stylistic/eslint-plugin": "^3.1.0",
    "defu": "^6.1.4",
    "eslint-config-flat-gitignore": "^2.0.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-de-morgan": "^1.0.1",
    "eslint-plugin-package-json": "^0.26.0",
    "eslint-plugin-perfectionist": "^4.9.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-regexp": "^2.7.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^15.15.0",
    "prettier": "^3.5.1",
    "typescript-eslint": "^8.24.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.4",
    "bumpp": "^10.0.3",
    "jiti": "^2.4.2",
    "lint-staged": "^15.4.3",
    "simple-git-hooks": "^2.11.1",
    "tsup": "^8.3.6",
    "type-fest": "^4.34.1",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "eslint": ">= 9.0.0"
  },
  "packageManager": "pnpm@10.4.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "simple-git-hooks"
    ]
  }
}
