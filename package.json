    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "typescript-eslint": "^8.23.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.1",
    "jiti": "^2.4.2",
