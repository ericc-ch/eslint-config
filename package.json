{
  "name": "@echristian/eslint-config",
  "version": "0.0.43",
  "description": "ericc-ch ESLint config",
  "keywords": [
    "eslint-config"
  ],
  "homepage": "https://github.com/ericc-ch/eslint-config",
  "bugs": "https://github.com/ericc-ch/eslint-config/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ericc-ch/eslint-config.git"
  },
  "author": "Erick Christian <erickchristian48@gmail.com>",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "default": "./dist/main.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint .",
    "prepack": "pnpm run build",
    "prepare": "simple-git-hooks",
    "release": "bumpp && pnpm publish --access public"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "pnpm run lint --fix"
  },
  "dependencies": {
    "@eslint-react/eslint-plugin": "^1.50.0",
    "@eslint/js": "^9.28.0",
    "@eslint/json": "^0.12.0",
    "@stylistic/eslint-plugin": "^4.4.0",
    "defu": "^6.1.4",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-de-morgan": "^1.2.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-package-json": "^0.33.2",
    "eslint-plugin-perfectionist": "^4.13.0",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-regexp": "^2.7.0",
    "eslint-plugin-unicorn": "^59.0.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.2.0",
    "prettier": "^3.5.3",
    "typescript-eslint": "^8.33.0"
  },
  "devDependencies": {
    "@types/eslint-plugin-jsx-a11y": "^6.10.0",
    "@types/node": "^22.15.29",
    "bumpp": "^10.1.1",
    "jiti": "^2.4.2",
    "lint-staged": "^16.1.0",
    "simple-git-hooks": "^2.13.0",
    "tsup": "^8.5.0",
    "type-fest": "^4.41.0",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "eslint": ">=9.0.0"
  },
  "packageManager": "pnpm@10.11.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "simple-git-hooks"
    ]
  }
}
